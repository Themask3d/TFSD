\n * Component for displaying and editing a specific tutorial.\r\n * Allows viewing and editing details of a selected tutorial.\r\n */\r\nexport default {\r\n  name: \"tutorial\",\r\n  data() {\r\n    return {\r\n      currentTutorial: null, // The currently selected tutorial object.\r\n      message: '' // Message to display any status or error information.\r\n\r\n    };\r\n  },\r\n  methods: {\r\n    /**\r\n     * Fetches a tutorial by its ID and sets it as the current tutorial.\r\n     * @param {number} id - The ID of the tutorial to fetch.\r\n     */\r\n    getTutorial(id) {\r\n      TutorialDataService.get(id)\r\n        .then(response => {\r\n          this.currentTutorial = response.data;\r\n          console.log(response.data);\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n    },\r\n\r\n    /**\r\n     * Updates the published status of the current tutorial.\r\n     * @param {boolean} status - The new published status to set.\r\n     */\r\n    updatePublished(status) {\r\n      var data = {\r\n        id: this.currentTutorial.id,\r\n        title: this.currentTutorial.title,\r\n        description: this.currentTutorial.description,\r\n        published: status\r\n      };\r\n\r\n      TutorialDataService.update(this.currentTutorial.id, data)\r\n        .then(response => {\r\n          console.log(response.data);\r\n          this.currentTutorial.published = status;\r\n          this.message = 'The status was updated successfully!';\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n    },\r\n\r\n    /**\r\n     * Updates the current tutorial details.\r\n     * Sends the current tutorial data to the backend for update.\r\n     */\r\n    updateTutorial() {\r\n      TutorialDataService.update(this.currentTutorial.id, this.currentTutorial)\r\n        .then(response => {\r\n          console.log(response.data);\r\n          this.message = 'The tutorial was updated successfully!';\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n    },\r\n\r\n    /**\r\n     * Deletes the current tutorial.\r\n     * Upon successful deletion, redirects to the tutorials list.\r\n     */\r\n    deleteTutorial() {\r\n      TutorialDataService.delete(this.currentTutorial.id)\r\n        .then(response => {\r\n          console.log(response.data);\r\n          this.$router.push({ name: \"tutorials\" });\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n    }\r\n  },\r\n  mounted() {\r\n    this.message = '';\r\n    this.getTutorial(this.$route.params.id);\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.edit-form {\r\n  max-width: 300px;\r\n  margin: auto;\r\n}\r\n</style>\r\n"],"mappings":";;;EAC8BA,KAAK,EAAC;;gCAChCC,mBAAA,CAAiB,YAAb,UAAQ;;EAELD,KAAK,EAAC;AAAY;gCACrBC,mBAAA,CAAgC;EAAzBC,GAAG,EAAC;AAAO,GAAC,OAAK;;EAKrBF,KAAK,EAAC;AAAY;gCACrBC,mBAAA,CAA4C;EAArCC,GAAG,EAAC;AAAa,GAAC,aAAW;;EAMjCF,KAAK,EAAC;AAAY;gCACrBC,mBAAA,CAAuC,gB,aAAhCA,mBAAA,CAAwB,gBAAhB,SAAO,E;;;;iCAgC1BA,mBAAA,CAAM;iCACNA,mBAAA,CAAoC,WAAjC,+BAA6B;qBADhCE,WAAM,EACNC,WAAoC,C;;SAlD3BC,KAAA,CAAAC,eAAe,I,cAA1BC,mBAAA,CA8CM,OA9CNC,UA8CM,GA7CJC,UAAiB,EACjBR,mBAAA,CAkBO,eAjBLA,mBAAA,CAKM,OALNS,UAKM,GAJJC,UAAgC,E,gBAChCV,mBAAA,CAEE;IAFKW,IAAI,EAAC,MAAM;IAACZ,KAAK,EAAC,cAAc;IAACa,EAAE,EAAC,OAAO;+DACvCR,KAAA,CAAAC,eAAe,CAACQ,KAAK,GAAAC,MAAA;gCAArBV,KAAA,CAAAC,eAAe,CAACQ,KAAK,E,KAGlCb,mBAAA,CAKM,OALNe,UAKM,GAJJC,UAA4C,E,gBAC5ChB,mBAAA,CAEE;IAFKW,IAAI,EAAC,MAAM;IAACZ,KAAK,EAAC,cAAc;IAACa,EAAE,EAAC,aAAa;+DAC7CR,KAAA,CAAAC,eAAe,CAACY,WAAW,GAAAH,MAAA;gCAA3BV,KAAA,CAAAC,eAAe,CAACY,WAAW,E,KAIxCjB,mBAAA,CAGM,OAHNkB,UAGM,GAFJC,UAAuC,E,iBAAA,GACvC,GAAAC,gBAAA,CAAGhB,KAAA,CAAAC,eAAe,CAACgB,SAAS,gC,KAKxBjB,KAAA,CAAAC,eAAe,CAACgB,SAAS,I,cADjCf,mBAAA,CAKS;;IALDP,KAAK,EAAC,0BAA0B;IAErCuB,OAAK,EAAAC,MAAA,QAAAA,MAAA,MAAAT,MAAA,IAAEU,QAAA,CAAAC,eAAe;KACxB,aAED,M,cACAnB,mBAAA,CAIS;;IAJMP,KAAK,EAAC,0BAA0B;IAC5CuB,OAAK,EAAAC,MAAA,QAAAA,MAAA,MAAAT,MAAA,IAAEU,QAAA,CAAAC