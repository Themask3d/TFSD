>\r\n        <div class=\"input-group-append\">\r\n          <button class=\"btn btn-outline-secondary\" type=\"button\"\r\n            @click=\"searchTitle\"\r\n          >\r\n            Search\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div class=\"col-md-6\">\r\n      <h4>Tutorials List</h4>\r\n      <ul class=\"list-group\">\r\n        <li class=\"list-group-item\"\r\n          :class=\"{ active: index == currentIndex }\"\r\n          v-for=\"(tutorial, index) in tutorials\"\r\n          :key=\"index\"\r\n          @click=\"setActiveTutorial(tutorial, index)\"\r\n        >\r\n          {{ tutorial.title }}\r\n        </li>\r\n      </ul>\r\n\r\n      <button class=\"m-3 btn btn-sm btn-danger\" @click=\"removeAllTutorials\">\r\n        Remove All\r\n      </button>\r\n    </div>\r\n    <div class=\"col-md-6\">\r\n      <div v-if=\"currentTutorial\">\r\n        <h4>Tutorial</h4>\r\n        <div>\r\n          <label><strong>Title:</strong></label> {{ currentTutorial.title }}\r\n        </div>\r\n        <div>\r\n          <label><strong>Description:</strong></label> {{ currentTutorial.description }}\r\n        </div>\r\n        <div>\r\n          <label><strong>Status:</strong></label> {{ currentTutorial.published ? \"Published\" : \"Pending\" }}\r\n        </div>\r\n\r\n        <router-link :to=\"'/tutorials/' + currentTutorial.id\" class=\"badge badge-warning\">Edit</router-link>\r\n      </div>\r\n      <div v-else>\r\n        <br />\r\n        <p>Please click on a Tutorial...</p>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport TutorialDataService from \"../services/TutorialDataService\";\r\n/**\r\n * Component for listing all tutorials and managing individual tutorials.\r\n * Enables viewing, searching, and deleting tutorials.\r\n */\r\nexport default {\r\n  name: \"tutorials-list\",\r\n  data() {\r\n    return {\r\n      tutorials: [], // Array to store the list of tutorials.\r\n      currentTutorial: null, // The currently selected or viewed tutorial.\r\n      currentIndex: -1, // Index of the currently selected tutorial in the list.\r\n      title: \"\" // Title used for searching tutorials.\r\n    };\r\n  },\r\n  methods: {\r\n    /**\r\n     * Retrieves all tutorials from the backend service.\r\n     */\r\n    retrieveTutorials() {\r\n      TutorialDataService.getAll()\r\n        .then(response => {\r\n          this.tutorials = response.data;\r\n          console.log(response.data);\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n    },\r\n    /**\r\n     * Refreshes the list of tutorials and resets the current tutorial.\r\n     */\r\n    refreshList() {\r\n      this.retrieveTutorials();\r\n      this.currentTutorial = null;\r\n      this.currentIndex = -1;\r\n    },\r\n\r\n    /**\r\n     * Sets the active tutorial based on user selection.\r\n     * @param {Object} tutorial - The tutorial object to set as current.\r\n     * @param {number} index - The index of the tutorial in the list.\r\n     */\r\n\r\n    setActiveTutorial(tutorial, index) {\r\n      this.currentTutorial = tutorial;\r\n      this.currentIndex = tutorial ? index : -1;\r\n    },\r\n\r\n    /**\r\n     * Removes all tutorials from the list.\r\n     */\r\n\r\n    removeAllTutorials() {\r\n      TutorialDataService.deleteAll()\r\n        .then(response => {\r\n          console.log(response.data);\r\n          this.refreshList();\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n    },\r\n\r\n    /**\r\n     * Searches for tutorials by title.\r\n     */\r\n    searchTitle() {\r\n      TutorialDataService.findByTitle(this.title)\r\n        .then(response => {\r\n          this.tutorials = response.data;\r\n          this.setActiveTutorial(null);\r\n          console.log(response.data);\r\n        })\r\n        .catch(e => {\r\n          console.log(e);\r\n        });\r\n    }\r\n  },\r\n  mounted() {\r\n    this.retrieveTutorials();\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n.list {\r\n  text-align: left;\r\n  max-width: 750px;\r\n  margin: auto;\r\n}\r\n</style>\r\n"],"mappings":"AAwDA,OAAOA,mBAAkB,MAAO,iCAAiC;AACjE;;;;AAIA,eAAe;EACbC,IAAI,EAAE,gBAAgB;EACtBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MAAE;MACfC,eAAe,EAAE,IAAI;MAAE;MACvBC,YAAY,EAAE,CAAC,CAAC;MAAE;MAClBC,KAAK,EAAE,EAAC,CAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP;;;IAGAC,iBAAiBA,CAAA,EAAG;MAClBR,mBAAmB,CAACS,MAAM,CAAC,EACxBC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACR,SAAQ,GAAIQ,QAAQ,CAACT,IAAI;QAC9BU,OAAO,CAACC,GAAG,CAACF,QAAQ,CAACT,IAAI,CAAC;MAC5B,CAAC,EACAY,KAAK,CAACC,CAAA,IAAK;QACVH,OAAO,CAACC,GAAG,CAACE,CAAC,CAAC;MAChB,CAAC,CAAC;IACN,CAAC;IACD;;;IAGAC,WAAWA,CAAA,EAAG;MACZ,IAAI,CAACR,iBAAiB,CAAC,CAAC;MACxB,IAAI,CAACJ,eAAc,GAAI,IAAI;MAC3B,IAAI,CAACC,YAAW,GAAI,CAAC,CAAC;IACxB,CAAC;IAED;;;;;;IAMAY,iBAAiBA,CAACC,QAAQ,EAAEC,KAAK,EAAE;MACjC,IAAI,CAACf,eAAc,GAAIc,QAAQ;MAC/B,IAAI,CAACb,YAAW,GAAIa,QAAO,GAAIC,KAAI,GAAI,CAAC,CAAC;IAC3C,CAAC;IAED;;;;IAIAC,kBAAkBA,CAAA,EAAG;MACnBpB,mBAAmB,CAACqB,SAAS,CAAC,EAC3BX,IAAI,CAACC,QAAO,IAAK;QAChBC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAACT,IAAI,CAAC;QAC1B,IAAI,CAACc,WAAW,CAAC,CAAC;MACpB,CAAC,EACAF,KAAK,CAACC,CAAA,IAAK;QACVH,OAAO,CAACC,GAAG,CAACE,CAAC,CAAC;MAChB,CAAC,CAAC;IACN,CAAC;IAED;;;IAGAO,WAAWA,CAAA,EAAG;MACZtB,mBAAmB,CAACuB,WAAW,CAAC,IAAI,CAACjB,KAAK,EACvCI,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACR,SAAQ,GAAIQ,QAAQ,CAACT,IAAI;QAC9B,IAAI,CAACe,iBAAiB,CAAC,IAAI,CAAC;QAC5BL,OAAO,CAACC,GAAG,CAACF,QAAQ,CAACT,IAAI,CAAC;MAC5B,CAAC,EACAY,KAAK,CAACC,CAAA,IAAK;QACVH,OAAO,CAACC,GAAG,CAACE,CAAC,CAAC;MAChB,CAAC,CAAC;IACN;EACF,CAAC;EACDS,OAAOA,CAAA,EAAG;IACR,IAAI,CAAChB,iBAAiB,CAAC,CAAC;EAC1B;AACF,CAAC"}]}